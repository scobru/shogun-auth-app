{
  "name": "shogun-music-app",
  "version": "1.0.0",
  "description": "Decentralized music platform",
  "main": "index.js",
  "scripts": {
    "start:storage": "node storage-relay/storage-relay.js",
    "start:metadata": "node metadata-relay/metadata-relay.js",
    "start:metadata2": "node metadata-relay/metadata2-relay.js",
    "start:identity": "node identity-relay/identity-relay.js",
    "start:all": "concurrently \"npm run start:storage\" \"npm run start:metadata\" \"npm run start:identity\"",
    "start:app": "node app-server.js",
    "start:static": "cd app && npx serve -s .",
    "start": "concurrently \"npm run start:all\" \"npm run start:app\""
  },
  "dependencies": {
    "axios": "^1.8.4",
    "concurrently": "^8.2.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "gun": "^0.2020.1239",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "serve": "^14.2.1"
  },
  "author": "Il Tuo Nome",
  "license": "MIT"
}
