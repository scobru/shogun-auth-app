{
  "name": "@shogun/stealth-address",
  "version": "1.0.0",
  "description": "Stealth Address plugin for Shogun Core - Provides privacy-focused stealth address functionality",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "prebuild": "rimraf dist"
  },
  "keywords": [
    "stealth",
    "privacy",
    "ethereum",
    "address",
    "crypto",
    "shogun"
  ],
  "author": "Scobru",
  "license": "MIT",
  "peerDependencies": {
    "shogun-core": "^1.1.4"
  },
  "dependencies": {
    "ethers": "^6.13.5",
    "gun": "^0.2020.1240"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
} 