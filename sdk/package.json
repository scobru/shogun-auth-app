{
  "name": "shogun-sdk",
  "version": "1.1.0",
  "description": "SHOGUN SDK - Libreria semplificata per la gestione di wallet crypto con GunDB",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "postbuild": "cp -r src/hedgehog dist/hedgehog"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "gun",
    "ethereum",
    "wallet",
    "crypto",
    "gunDB"
  ],
  "author": "Scpbru",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@audius/hedgehog": "file:../hedgehog",
    "browserify-cipher": "1.0.1",
    "ethereumjs-wallet": "^1.0.2",
    "ethers": "^6.13.5",
    "gun": "^0.2020.1240",
    "react": "^19.0.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  }
}
