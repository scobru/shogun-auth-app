{
  "name": "@shogun/shogun-core",
  "version": "1.1.0",
  "description": "SHOGUN SDK - Libreria semplificata per la gestione di wallet crypto con GunDB",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "format": "npx prettier --write 'src/**/*.ts'"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "gun",
    "ethereum",
    "wallet",
    "crypto",
    "gunDB"
  ],
  "author": "Scobru",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@audius/hedgehog": "latest",
    "base64url": "^3.0.1",
    "browserify-cipher": "1.0.1",
    "ethereumjs-wallet": "^1.0.2",
    "ethers": "^6.11.1",
    "gun": "git+https://github.com/scobru/gun.git",
    "jsx": "^0.9.89",
    "jsxs": "^1.0.1",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^18.2.0",
    "solid-js": "^1.8.15",
    "typescript": "^5.3.3",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^18.2.19",
    "typescript": "^5.3.3"
  }
}
